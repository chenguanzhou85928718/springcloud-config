<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.gradledemo.mapper.SelectedcourseMapper">

    <resultMap id="BaseResultMap" type="com.example.gradledemo.domain.Selectedcourse">
            <result property="courseid" column="courseID" jdbcType="INTEGER"/>
            <result property="studentid" column="studentID" jdbcType="INTEGER"/>
            <result property="mark" column="mark" jdbcType="INTEGER"/>
            <result property="createTime" column="create_time" jdbcType="TIMESTAMP"/>
            <result property="updateTime" column="update_time" jdbcType="TIMESTAMP"/>
            <result property="isDelete" column="is_delete" jdbcType="VARCHAR"/>
    </resultMap>

    <sql id="Base_Column_List">
        courseID,studentID,mark,
        create_time,update_time,is_delete
    </sql>
</mapper>
