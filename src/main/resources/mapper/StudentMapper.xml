<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.gradledemo.mapper.StudentMapper">

    <resultMap id="BaseResultMap" type="com.example.gradledemo.domain.Student">
            <id property="userid" column="userID" jdbcType="INTEGER"/>
            <result property="username" column="userName" jdbcType="VARCHAR"/>
            <result property="sex" column="sex" jdbcType="VARCHAR"/>
            <result property="birthyear" column="birthYear" jdbcType="DATE"/>
            <result property="grade" column="grade" jdbcType="DATE"/>
            <result property="collegeid" column="collegeID" jdbcType="INTEGER"/>
            <result property="createTime" column="create_time" jdbcType="TIMESTAMP"/>
            <result property="updateTime" column="update_time" jdbcType="TIMESTAMP"/>
            <result property="isDelete" column="is_delete" jdbcType="VARCHAR"/>
    </resultMap>

    <sql id="Base_Column_List">
        userID,userName,sex,
        birthYear,grade,collegeID,
        create_time,update_time,is_delete
    </sql>
</mapper>
